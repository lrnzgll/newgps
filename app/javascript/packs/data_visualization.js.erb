import axios from 'axios-on-rails'

axios.get('pages/data').then((response) => {
  var data = response.data.data
  
  document.addEventListener("DOMContentLoaded",function(){
    d3.select(".data_routes_by_region")
      .selectAll("div")
      .data(data[1])
      .enter()
      .append("div")
      .style("width", function(d) { return 2*d + "px"; })
      .style("height", "20px" )
      .style("margin", "8px")
      .text(function(d) { return 3*d; });
});
})


